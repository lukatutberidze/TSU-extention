{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "A brief description of my extension",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://uni.tsu.ge/*"],
      "js": ["main.js"],
      "css": ["styles.css"]
    }
  ]
}
